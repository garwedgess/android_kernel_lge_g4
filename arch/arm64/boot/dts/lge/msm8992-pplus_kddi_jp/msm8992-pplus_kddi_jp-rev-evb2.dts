/dts-v1/;

#include "../../qcom/msm8992.dtsi"
#include "msm8992-pplus_kddi_jp.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8992 PPLUS KDDI JP";
	compatible = "qcom,msm8992-cdp", "qcom,msm8992", "qcom,cdp";
	qcom,board-id = <0x165 0>;
};

//ADD HERE TO SUPPORT HW CHANGE

&soc {
	i2c_5: i2c@f9967000 {
		ti-tas2552_41@41 {
			status = "ok";
		};
	};
};

/* fingerprint spi master */
&soc {
	spi_blsp12: spi_fpc@f9968000{
		status = "disabled";
	};
};

/* fingerprint fpc1022 */
&spi_blsp12 {
	csfp@0 {
		status = "disabled";
	};
};

/* fingerprint spi master */
&soc {
	spi_blsp10: spi_fpc@f9966000{
		status = "ok";
	};
};

/* fingerprint fpc1022 */
&spi_blsp10 {
	csfp@0 {
		status = "ok";
	};
};
